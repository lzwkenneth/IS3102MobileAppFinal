    <ImageView
        android:id="@+id/infoIconb"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tttttb"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="12dp"
        android:src="@drawable/ic_info_outline_black_24dp" />

    <TextView
        android:id="@+id/eventTitle123b"
        android:layout_marginLeft="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:layout_below="@+id/tttttb"
        android:layout_marginTop="11dp"
        android:layout_toEndOf="@+id/infoIconb" />

    <ImageView
        android:id="@+id/descIconb"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/infoIconb"
        android:layout_below="@+id/infoIconb"
        android:layout_marginTop="18dp"
        android:src="@drawable/ic_description_black_24dp" />

    <TextView
        android:id="@+id/eventDescriptionb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/descIconb"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="11dp"
        android:textSize="14dp"
        android:layout_toEndOf="@+id/descIconb" />

    <ImageView
        android:id="@+id/locationb"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="18dp"
        android:src="@drawable/ic_place_black_24dp"
        android:layout_below="@+id/descIconb"
        android:layout_alignStart="@+id/descIconb" />





    <TextView
        android:text="Loading building image..."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:id="@+id/textLoadingb" /><![CDATA[

    "
        android:layout_alignParentEnd="true" />







    ]]>

    <TextView
        android:id="@+id/eventAddressb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:textSize="14dp"
        android:layout_marginBottom="3dp"
        android:layout_alignBottom="@+id/locationb"
        android:layout_alignStart="@+id/eventDescriptionb" />

    <ImageView
        android:id="@+id/tttttb"
        android:layout_width="match_parent"
        android:layout_height="300dip"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

